        "line": {
          "hover": "#181825",
          "regular": "#181825",
          "unfocused": "#181825"
        },
        "text": "#a6adc8"
      },
      "selected": {
        "backgrounds": {
          "hover": "#313244",
          "regular": "#313244",
          "unfocused": "#313244"
        },
        "line": {
          "hover": "#f5c2e7",
          "regular": "#f5c2e7",
          "unfocused": "#f5c2e7"
        },
        "text": "#cdd6f4"
      }
