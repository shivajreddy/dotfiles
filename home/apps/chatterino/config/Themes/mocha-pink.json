{
  "$schema": "https://raw.githubusercontent.com/Chatterino/chatterino2/master/docs/ChatterinoTheme.schema.json",
  "colors": {
    "accent": "#f5c2e7",
    "messages": {
      "backgrounds": {
        "alternate": "#1e1e2e",
        "regular": "#181825"
      },
      "disabled": "#9911111b",
      "highlightAnimationEnd": "#009399b2",
      "highlightAnimationStart": "#6e9399b2",
      "selection": "#40cdd6f4",
      "textColors": {
        "caret": "#cdd6f4",
        "chatPlaceholder": "#bac2de",
        "link": "#f5c2e7",
        "regular": "#cdd6f4",
        "system": "#a6adc8"
      }
    },
    "scrollbars": {
      "background": "#0011111b",
      "thumb": "#7f849c",
      "thumbSelected": "#6c7086"
    },
    "splits": {
      "background": "#11111b",
      "dropPreview": "#30f5c2e7",
      "dropPreviewBorder": "#f5c2e7",
      "dropTargetRect": "#00f5c2e7",
      "dropTargetRectBorder": "#00f5c2e7",
      "header": {
        "background": "#181825",
        "border": "#11111b",
        "focusedBackground": "#181825",
        "focusedBorder": "#11111b",
        "focusedText": "#cdd6f4",
        "text": "#cdd6f4"
      },
      "input": {
        "background": "#181825",
        "text": "#f5c2e7"
      },
      "messageSeperator": "#313244",
      "resizeHandle": "#f5c2e7",
      "resizeHandleBackground": "#f5c2e7"
    },
    "tabs": {
      "dividerLine": "#f5c2e7",
